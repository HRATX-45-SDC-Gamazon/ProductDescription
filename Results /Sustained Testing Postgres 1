
config:
  target: "http://localhost:7777"
  phases:
    - duration: 60
      arrivalRate: 10
      rampTo: 100
    - duration: 120
      arrivalRate: 100
      rampTo: 500
    - duration: 60
      arrivalRate: 500
      name: "sustained heavy load"
  defaults:
    headers:
scenarios:
  - flow:
      - get:
          url: "/itemIdPG/?id={{$randomNumber(1, 10000000)}}"
 
